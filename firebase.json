{
  "hosting": {
    "public": "dist",
    "predeploy": "npm run build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "site": "devmian",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  }
}
